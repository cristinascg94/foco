{
  "version": 2,
  "builds": [
    {
      "src": "focoProject/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.10"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "focoProject/wsgi.py"
    }
  ],
  "functions": {
    "app/api/**/*": {
      "maxDuration": 59 
    }
  }
}