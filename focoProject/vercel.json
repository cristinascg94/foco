{
  "version": 2,
  "builds": [
    {
      "src": "focoProject/wsgi.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "focoProject/wsgi.py"
    }
  ],
  "functions": {
    "api/*.py": {
      "runtime": "python3.10",
      "maxLambdaSize": "15mb"
    }
  }
}
